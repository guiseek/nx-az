{
  "npmScope": "nx-az",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e", "compodoc"]
      }
    }
  },
  "projects": {
    "exp-app": {
      "tags": []
    },
    "exp-app-e2e": {
      "tags": [],
      "implicitDependencies": ["exp-app"]
    },
    "a11y-focus": {
      "tags": ["scope:a11y"]
    },
    "a11y-forms": {
      "tags": ["scope:a11y"]
    },
    "util-types": {
      "tags": ["scope:util"]
    },
    "fire-auth": {
      "tags": ["scope:fire"]
    },
    "doc-app": {
      "tags": ["type:app"]
    },
    "seek-cli": {
      "tags": ["scope:seek", "platform:cli"]
    }
  }
}
